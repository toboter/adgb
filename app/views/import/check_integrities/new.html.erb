<table class="table table-responsive table-hover">
  <thead>
    <tr>
      <th>Index</th>
      <% @items.first.keys.each do |k| %>
        <th><%= k %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @items.each_with_index do |i,index| %>
      <tr>
        <td><%= index+1 %></td>
        <% i.values.each do |v| %>
          <td><%= v %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>